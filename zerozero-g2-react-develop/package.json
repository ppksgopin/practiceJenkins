  "name": "learn_webpack",
  "version": "1.0.0",
  "description": "johnson learnning webpack",
  "main": "index.js",
  "scripts": {
    "start": "webpack",
  
  "postbuild": "cp robots.txt build/",
    "dev:client": "webpack-dev-server --open",
    "dev:server": "NODE_ENV=staging nodemon $NODE_DEBUG_OPTION --watch src src/server/main.js",
    "build:server": "rm -rf dist/ && cp -R src/ dist/ && cp -a static/* public/ && babel -d dist src",
    "build:client:dev": "rm -rf public/ && NODE_ENV=development webpack --progress",
    "build:client:stage": "rm -rf public/ && NODE_ENV=staging webpack --progress",
    "build:client:prod": "rm -rf public/ && NODE_ENV=production webpack --progress",
    "build:dev": "npm run  build:client:dev && npm run  build:server",
    "build:stage": "npm run build:client:stage && npm run build:server ",
    "build:prod": "npm run  build:client:prod && npm run  build:server",
    "build": "yarn build:client:prod && yarn build:server",
    "buildNpm": "npm run build:client && npm run build:server"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Johnson Hsu",
  "license": "ISC",
